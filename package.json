{
  "name": "galosity",
  "version": "1.0.0",
  "main": "./js/main.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "build": "cross-env NODE_ENV=production electron-builder",
    "grammar": "npx pegjs -o ./js/vars/grammar.js ./js/vars/grammar.pegjs",
    "tsc": "tsc"
  },
  "keywords": [],
  "author": "ZZ_404",
  "license": "MIT",
  "description": "Lite Pure-Text Galgame Editor",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "@types/electron": "^1.4.38",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "cross-env": "^7.0.3",
    "electron": "^35.2.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.30.1",
    "eslint-plugin-no-floating-promise": "^2.0.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.3.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "build": {
    "appId": "galosity.zz_404",
    "productName": "Galosity",
    "directories": {
      "output": "build"
    },
    "files": [
      "**/*",
      "!example.txt",
      "!gal.txt",
      "!README.md",
      "!LICENSE.txt",
      "!sum.txt",
      "!eslint.config.js",
      "!src",
      "!save",
      "!import-test.txt",
      "!ts"
    ],
    "extraFiles": [
      "example.txt",
      "src",
      "save"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "mathjax": "^3.2.2",
    "pegjs": "^0.10.0"
  }
}
