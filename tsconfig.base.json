{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "useUnknownInCatchVariables": true,
    "noImplicitOverride": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false
  }
}